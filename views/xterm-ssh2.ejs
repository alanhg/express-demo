  function updateSearchResult(result = [], keyword) {
    const elementsByClassNameElement = document.getElementsByClassName('search-result')[0];
    if (result.length === 0) {
      elementsByClassNameElement.innerHTML = `无相关内容`;
      return;
    }
    elementsByClassNameElement.innerHTML = `<ul>${
      result.map(item => `<li><a data-path='${item[0]}' data-row='${item[1]}' data-keyword='${keyword}'>${item[0]};${item[1]};${item[2]}</a></li>`).join('')
    }</ul>`;
    elementsByClassNameElement.addEventListener('click', (event) => {
      if (event.target.tagName === 'A') {
        searchResultItemOnClick(event);
      }
    });
  }
</script>

<script>
  function joinPath(l, r) {
    return `${l}/${r}`.replace(/\/{2,}/g, '/');
  }

  // 树JS文档 https://github.com/daweilv/treejs
  const sftpSocket = new WebSocket(`ws://127.0.0.1:8000/ws/sftp`);
  const rootPath = '/home/lighthouse';
  let typeMode;

  // 文件树
  let zTreeObj;
  let zTreeNodeSelectedId = rootPath;
  // 文本编辑器
  let codeEditor;

  function handleSearch(event) {
    if (event.key === 'Enter') {
      socket.send(JSON.stringify({
        type: 'search',
        path: zTreeNodeSelectedId,
        data: event.target.value
      }))
    }
  }

  function myOnClick(event, treeId, treeNode) {
    typeMode = 'get-file';
    if (treeNode.type === '-') {
      sftpSocket.send(JSON.stringify({
        type: 'get-file',
        path: treeNode.id
      }));
    }
    zTreeNodeSelectedId = treeNode.id;
  }

  function zTreeOnExpand(event, treeId, treeNode) {
    sftpSocket.send(JSON.stringify({
      type: 'list',
      path: treeNode.id
    }));
  }

  $(document).ready(function () {
    zTreeObj = $.fn.zTree.init($("#file-tree"), {
      callback: {
        onClick: myOnClick,
        onExpand: zTreeOnExpand
      }
    }, [
      {
        id: rootPath,
        name: rootPath,
        children: [],
        type: 'd'
      }
    ]);
  });

  function parseData(evt) {
    try {
      return JSON.parse(evt.data);
    } catch {
      return evt.data;
    }
  }


  sftpSocket.onopen = function () {

  }
  sftpSocket.onmessage = function (evt) {
    if (typeMode === 'get-file' && evt.data instanceof Blob) {
      evt.data.text().then((value) => {
        codeEditor.getModel().setValue(value);
      })
      console.log(evt.data.toString());
      return;
    }

    if (typeMode === 'download-file' && evt.data instanceof Blob) {
      saveFile(evt.data, window.term.getSelection());
      return;
    }

    const opts = JSON.parse(evt.data)
    if (opts.type === 'connected') {
      sftpSocket.send(JSON.stringify({
        type: 'list',
        path: rootPath
      }));
    }
    if (opts.type === 'list') {
      const parentNode = zTreeObj.getNodeByParam('id', opts.path, null);
      zTreeObj.removeChildNodes(parentNode);
      zTreeObj.addNodes(parentNode, opts.data.map(item => ({
        id: joinPath(opts.path, item.name),
        name: item.name,
        children: item.type === 'd' ? [] : null,
        type: item.type
      })));
    }
    if (opts.type === 'put-file') {
      alert(`${zTreeNodeSelectedId} saved!`);
    }
  }


