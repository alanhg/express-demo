  function Encodeuint8arr(myString) {
    return new TextEncoder("utf-8").encode(myString);
  }

  function Decodeuint8arr(uint8array) {
    return new TextDecoder("utf-8").decode(uint8array);
  }

  /**
   * 获取文件内容，同时高亮命中文本，滚动到指定位置
