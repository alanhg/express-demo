    <style>

        /**
          search-addon-bar样式
         */
        .xterm-search-addon-bar {
            position: absolute;
            right: 0;
            top: 0;
            width: 285px;
            color: #000;
            padding: 5px 10px;
            box-shadow: 0 2px 8px #000;
            background: #252526;
